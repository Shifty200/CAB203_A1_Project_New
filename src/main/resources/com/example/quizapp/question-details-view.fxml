<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.quizapp.controller.QuestionDetailsController"
      prefHeight="400.0" prefWidth="600.0">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <AnchorPane>
        <Label fx:id="quizName" AnchorPane.leftAnchor="10"></Label>
        <Button fx:id="backToResultsButton" AnchorPane.rightAnchor="10" onAction="#backToResults">Back to results</Button>
    </AnchorPane>
    <VBox fx:id="questions" alignment="CENTER"></VBox>
</VBox>
